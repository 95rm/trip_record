<div class="container">
 <%= form_with model: @review, url: trip_plan_detail_reviews_path, method: :patch do |f| %>
  <p>・投稿写真</p>
  <%#= image_tag @review.get_image(300,300) %>

  <p>・レビュー内容</p>
  <div class="row">
   <%= f.text_area :review %>
  </div>

  <div class="row mt-2">
   <%= f.submit '更新', class: "btn btn-success" %>
  </div>
 <% end %>

<div class="row mt-2 justify-content-center">
 <%= link_to "戻る", trip_plan_detail_reviews_path, class: "btn btn-success" %>
</div>

</div>